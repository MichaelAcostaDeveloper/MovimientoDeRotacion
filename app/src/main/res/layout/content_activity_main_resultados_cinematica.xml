<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Resultados.Resultados_Cinematica"
    tools:showIn="@layout/activity_main_resultados_cinematica">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/primera"
                android:layout_width="match_parent"
                android:layout_height="90dp">

                <TextView
                    android:id="@+id/m1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="35dp"
                    android:gravity="center"
                    android:text="Magnitud fisica descricion"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/imagen1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="18dp"
                    android:layout_marginEnd="35dp"
                    app:backgroundTint="@color/naranjaMedio"
                    app:srcCompat="@drawable/flecha_derecha" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/segunda"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@+id/primera">

                <TextView
                    android:gravity="center"
                    android:id="@+id/textResultado1"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="85dp"
                    android:layout_marginTop="5dp"
                    android:text="nnnnnnn"
                    android:textColor="#000000"
                    android:textSize="17dp" />

                <TextView
                    android:id="@+id/i1"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="45dp"
                    android:text="unidades"
                    android:textColor="#000000"
                    android:textSize="17dp" />

            </RelativeLayout>

            <View
                android:id="@+id/linea1"
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:layout_below="@+id/segunda"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:background="#c0c0c0"
                android:padding="10dp" />

            <RelativeLayout
                android:id="@+id/tercera"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_below="@+id/linea1">

                <TextView
                    android:id="@+id/m2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="35dp"
                    android:gravity="center"
                    android:text="Magnitud fisica descricion"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/imagen2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="18dp"
                    android:layout_marginEnd="35dp"
                    android:foregroundGravity="center"
                    app:backgroundTint="@color/naranjaMedio"
                    app:srcCompat="@drawable/flecha_derecha" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/cuarta"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@+id/tercera">

                <TextView
                    android:id="@+id/textResultado2"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="85dp"
                    android:layout_marginTop="5dp"
                    android:text="nnnnnnn"
                    android:textColor="#000000"
                    android:textSize="17dp" />

                <TextView
                    android:id="@+id/i2"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="45dp"
                    android:text="unidades"
                    android:textColor="#000000"
                    android:textSize="17dp" />


            </RelativeLayout>

            <View
                android:id="@+id/linea2"
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:layout_below="@+id/cuarta"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:background="#c0c0c0"
                android:padding="10dp" />

            <RelativeLayout
                android:id="@+id/quinta"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_below="@+id/linea2">

                <TextView
                    android:id="@+id/m3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="35dp"
                    android:gravity="center"
                    android:text="Magnitud fisica descricion"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/imagen3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="18dp"
                    android:layout_marginEnd="35dp"
                    android:foregroundGravity="center"
                    app:backgroundTint="@color/naranjaMedio"
                    app:srcCompat="@drawable/flecha_derecha" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/sexta"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@+id/quinta">


                <TextView
                    android:id="@+id/textResultado3"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:layout_marginStart="85dp"
                    android:layout_marginTop="5dp"
                    android:text="nnnnnnn"
                    android:textColor="#000000"
                    android:textSize="17dp" />

                <TextView
                    android:id="@+id/i3"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="45dp"
                    android:text="unidades"
                    android:textColor="#000000"
                    android:textSize="17dp" />


            </RelativeLayout>

            <View
                android:id="@+id/linea3"
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:layout_below="@+id/sexta"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:background="#c0c0c0"
                android:padding="10dp" />

            <RelativeLayout
                android:id="@+id/septima"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_below="@+id/linea3">

                <TextView
                    android:id="@+id/m4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="35dp"
                    android:gravity="center"
                    android:text="Magnitud fisica descricion"
                    android:textColor="#000000"
                    android:textSize="18dp"
                    android:textStyle="bold" />

                <android.support.design.widget.FloatingActionButton
                    android:id="@+id/imagen4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="18dp"
                    android:layout_marginEnd="35dp"
                    android:foregroundGravity="center"
                    app:backgroundTint="@color/naranjaMedio"
                    app:srcCompat="@drawable/flecha_derecha" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/octava"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@+id/septima">

                <TextView
                    android:id="@+id/textResultado4"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_marginStart="85dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="nnnnnnn"
                    android:textColor="#000000"
                    android:textSize="17dp" />

                <TextView
                    android:id="@+id/i4"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:gravity="center"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="45dp"
                    android:text="unidades"
                    android:textColor="#000000"
                    android:textSize="17dp" />


            </RelativeLayout>


        </LinearLayout>
    </ScrollView>


</LinearLayout>



